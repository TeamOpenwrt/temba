# put 'y' if you want to update feeds each time imagebuilder.sh starts
syncfeeds='n'

# put 'y' if you want to install custom package dtun
dtun='n'

# put 'y' if you want to install custom patches
patches='n'

# put 'y' if you want to enable "compliance test" mode
compliance_test='y'

# select openwrt version (do `cd Openwrt; git tag` to select another one)
openwrt_version='v18.06.4'

# select ar71xx, x86_64 or both
archs=(
  ar71xx
  #ath79
  #x86_64
)
